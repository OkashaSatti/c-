#include<iostream>
using namespace std;
int main()
{
    int subject,marks,total;
    cout<<"Input number of subjects: ";
    cin>>sub;
    for(int i=1; i<=subject; i++)
    {
        cout<<"Enter marks of Subject "<<i<<" : ";
        cin>>marks;
        total=total+marks;
    }
    cout<<"The total marks are: "<<total<<endl;
    if(total >= 90)
    cout<<"Grade: A"<<endl;
    else if(total <=89)
    cout<<"Grade: B"<<endl;
    else if(total <= 74)
    cout<<"Grade: C"<<endl;
    else if(total <=50)
    cout<<"Grade: F"<<endl;

    cout<<"The average of total is: "<<total/subject;
}
